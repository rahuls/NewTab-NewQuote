{
    "manifest_version": 2,
    "name": "New Tab New Quote",
    "description": "Shows a new Quote on each new tab.",
    "version": "1.0",
    "chrome_url_overrides" : {
        "newtab": "mytab.html"
    },
    "browser_action": {
        "default_icon": "icon16.png"
      },
    "icons": { "16": "icon16.png",
               "48": "icon48.png",
              "128": "icon128.png" }
}